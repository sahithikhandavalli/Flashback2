<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pro.diary.dell.diary" >
    <uses-sdk android:targetSdkVersion="21" android:minSdkVersion="8"/>

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="pro.diary.dell.diary.ActivityMain"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="pro.diary.dell.diary.ActivityDiaryEdit"
            android:label="ActivityDiaryEdit"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="pro.diary.dell.diary.Camera1"
            android:label="@string/title_activity_camera1"
            android:parentActivityName="pro.diary.dell.diary.Options"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="pro.diary.dell.diary.Options" />
        </activity>
        <activity
            android:name="pro.diary.dell.diary.VView"
            android:label="@string/title_activity_vview"
            android:parentActivityName="pro.diary.dell.diary.Camera1"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="pro.diary.dell.diary.Camera1" />
        </activity>
        <activity
            android:name="pro.diary.dell.diary.Options"
            android:label="@string/title_activity_options"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="pro.diary.dell.diary.Video"
            android:label="@string/title_activity_video" >
        </activity>
    </application>

</manifest><!--
